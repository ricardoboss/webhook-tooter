{
    "name": "ricardoboss/webhook-tooter",
    "description": "Validating and parsing of GitHub webhook requests and posting to Twitter",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ricardo Boss",
            "email": "contact@ricardoboss.de"
        }
    ],
    "require": {
        "php": "^8.1",
        "coderjerk/bird-elephant": "^0.4",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "nyholm/psr7": "^1.5",
        "mockery/mockery": "^1.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "ricardoboss\\WebhookTooter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ricardoboss\\WebhookTooter\\": "test/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
